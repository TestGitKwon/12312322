-- 관리자 페이지 테이블 추가
CREATE USER DOIT IDENTIFIED BY DOIT;

GRANT RESOURCE, CONNECT TO DOIT;



-- DOIT 테이블 멤버관련 쿼리문
DROP TABLE DOIT_MEMBER CASCADE CONSTRAINTS;

CREATE TABLE DOIT_MEMBER (
    MID NUMBER UNIQUE
    ,USERID VARCHAR2(30)
    ,USERPWD VARCHAR2(100)
    ,USERNAME VARCHAR2(30)
);

commit;

INSERT INTO DOIT_MEMBER VALUES (
    1, 'test', '03ac674216f3e15c761ee1a5e255f067953623c8b388b4459e13f978d7c846f4', '테스트'
);

SELECT  *
FROM    DOIT_MEMBER
;

commit;